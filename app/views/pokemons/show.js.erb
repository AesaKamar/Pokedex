$('#def').width('<%= j ((@pokemon.def*100/255).to_s) %>%')
$('#spatk').width('<%= j ((@pokemon.spatk*100/255).to_s) %>%')
$('#spdef').width('<%= j ((@pokemon.spdef*100/255).to_s) %>%')
$('#spe').width('<%= j ((@pokemon.spe*100/255).to_s) %>%')
