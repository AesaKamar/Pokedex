$('div#ajaxImg').html("<%= j (image_tag @pokemon.name.downcase + ".gif", :class => "lgImg") %>");


$('img.lgImg').load(function(){
    var height, width;
    height = $('img.lgImg').height();
    width = $('img.lgImg').width();
    console.log(width, height);
    var scaling = 2;

    $('img.lgImg').css({height : scaling*height});

});


$('#hp').width('<%= j ((@pokemon.baseStats[0]*100/255).to_s) %>%')
$('#atk').width('<%= j ((@pokemon.baseStats[1]*100/255).to_s) %>%')
$('#def').width('<%= j ((@pokemon.baseStats[2]*100/255).to_s) %>%')
$('#spatk').width('<%= j ((@pokemon.baseStats[3]*100/255).to_s) %>%')
$('#spdef').width('<%= j ((@pokemon.baseStats[4]*100/255).to_s) %>%')
$('#spe').width('<%= j ((@pokemon.baseStats[5]*100/255).to_s) %>%')
